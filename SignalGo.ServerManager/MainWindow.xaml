<Window x:Class="SignalGo.ServerManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SignalGo.ServerManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
		<StackPanel VerticalAlignment="Center">
			<Button x:Name="btnLoad" Content="Load" Click="btnLoad_Click"/>
			<Button x:Name="btnUnLoad" Content="UnLoad" Click="btnUnLoad_Click"/>
		</StackPanel>
	</Grid>
</Window>
